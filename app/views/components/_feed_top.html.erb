<div class="feed_top">
  <% unless current_user.location.nil? %>
    <h3><%= current_user.geocoder_object["address"]["city_district"] %></h3>
  <% end %>
  <p> <strong><%= users.length - 1 %> travelpals</strong> around you </p>
    <div class="dropdown feed-top-dropdown">
      <form method="get" action="/">
        <select name="radius" onChange="this.form.submit()">
          <option value="50">50 km radius</option>
          <option value="0">0 km radius</option>
          <option value="5">5 km radius</option>
          <option value="10">10 km radius</option>
          <option value="25">25 km radius</option>
        </select>
      </form>
    </div>
</div>
