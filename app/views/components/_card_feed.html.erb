  <div class="Feed-card">
    <div class="Feed-card-header">
      <%= cl_image_tag user.avatar.key, class: "Feed-card-user-image"%>
      <a class="Feed-card-user-name" href="__"> <%= user.username %> </a>
      <div class="Feed-card-location">
        <i class="fa-solid fa-location-dot"></i>
        <strong><%= user.trips.last.moments.last.nearest_city%></strong>, 1km
      </div>
    </div>
    <div class="Feed-card-image">
      <%= cl_image_tag user.trips.last.moments.last.photo.key, class: "Feed-card-location-image"%>
    </div>
  <div class="Feed-card">
