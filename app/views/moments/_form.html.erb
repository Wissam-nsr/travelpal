<div class='container-stef'>
<%= simple_form_for [@moment], url: new_moment_path do |f| %>
  <%= f.input :description, input_html: { style:'background: white' } %>
  <%= f.input :location, input_html: { style:'background: white' } %>
  <%= f.input :photo, as: :file %>
  <br>
  <%= f.button :submit, class: "btn btn-dark" %>
<% end %>
</div>
